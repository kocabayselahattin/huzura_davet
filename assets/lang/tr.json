  "verses": [
    {"text": "Şüphesiz namaz, hayâsızlıktan ve kötülükten alıkoyar.", "source": "Ankebût, 45"},
    {"text": "Sabır ve namaz ile Allah'tan yardım isteyin.", "source": "Bakara, 45"},
    {"text": "O, göklerin ve yerin nurudur.", "source": "Nûr, 35"},
    {"text": "Allah'ı çokça zikredin ki kurtuluşa eresiniz.", "source": "Cuma, 10"},
    {"text": "Bana dua edin, size karşılık vereyim.", "source": "Mü'min, 60"},
    {"text": "Muhakkak ki zorlukla beraber kolaylık vardır.", "source": "İnşirah, 6"},
    {"text": "Kim Allah'a tevekkül ederse, O ona yeter.", "source": "Talâk, 3"},
    {"text": "Rabbiniz'den mağfiret dileyin. O çok bağışlayıcıdır.", "source": "Nûh, 10"},
    {"text": "Namazı dosdoğru kılın, zekâtı verin.", "source": "Bakara, 43"},
    {"text": "Allah sabredenleri sever.", "source": "Âl-i İmrân, 146"},
    {"text": "Rahmetim her şeyi kuşatmıştır.", "source": "A'râf, 156"},
    {"text": "O, kullarına karşı çok şefkatlidir.", "source": "Şûrâ, 19"},
    {"text": "Rabbim! Beni namaza devam eden kıl.", "source": "İbrâhîm, 40"},
    {"text": "Güzel söz O'na yükselir.", "source": "Fâtır, 10"},
