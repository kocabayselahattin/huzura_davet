  "verses": [
    {"text": "إِنَّ الصَّلَاةَ تَنْهَىٰ عَنِ الْفَحْشَاءِ وَالْمُنكَرِ", "source": "العنكبوت، 45"},
    {"text": "وَاسْتَعِينُوا بِالصَّبْرِ وَالصَّلَاةِ", "source": "البقرة، 45"},
    {"text": "اللَّهُ نُورُ السَّمَاوَاتِ وَالْأَرْضِ", "source": "النور، 35"},
    {"text": "وَاذْكُرُوا اللَّهَ كَثِيرًا لَّعَلَّكُمْ تُفْلِحُونَ", "source": "الجمعة، 10"},
    {"text": "ادْعُونِي أَسْتَجِبْ لَكُمْ", "source": "غافر، 60"},
    {"text": "إِنَّ مَعَ الْعُسْرِ يُسْرًا", "source": "الشرح، 6"},
    {"text": "وَمَن يَتَوَكَّلْ عَلَى اللَّهِ فَهُوَ حَسْبُهُ", "source": "الطلاق، 3"},
    {"text": "اسْتَغْفِرُوا رَبَّكُمْ إِنَّهُ كَانَ غَفَّارًا", "source": "نوح، 10"},
    {"text": "وَأَقِيمُوا الصَّلَاةَ وَآتُوا الزَّكَاةَ", "source": "البقرة، 43"},
    {"text": "وَاللَّهُ يُحِبُّ الصَّابِرِينَ", "source": "آل عمران، 146"},
    {"text": "وَرَحْمَتِي وَسِعَتْ كُلَّ شَيْءٍ", "source": "الأعراف، 156"},
    {"text": "وَهُوَ اللَّطِيفُ بِعِبَادِهِ", "source": "الشورى، 19"},
    {"text": "رَبِّ اجْعَلْنِي مُقِيمَ الصَّلَاةِ", "source": "إبراهيم، 40"},
    {"text": "إِلَيْهِ يَصْعَدُ الْكَلِمُ الطَّيِّبُ", "source": "فاطر، 10"},
    {"text": "إِذَا جَاءَ نَصْرُ اللَّهِ وَالْفَتْحُ", "source": "النصر، 1"},
    {"text": "وَقُل رَّبِّ زِدْنِي عِلْمًا", "source": "طه، 114"},
    {"text": "وَإِن تَعُدُّوا نِعْمَةَ اللَّهِ لَا تُحْصُوهَا", "source": "النحل، 18"},
    {"text": "يُرِيدُ اللَّهُ بِكُمُ الْيُسْرَ", "source": "البقرة، 185"},
    {"text": "إِنَّ اللَّهَ يَأْمُرُ بِالْعَدْلِ", "source": "النحل، 90"},
    {"text": "وَعِبَادُ الرَّحْمَٰنِ الَّذِينَ يَمْشُونَ عَلَى الْأَرْضِ هَوْنًا", "source": "الفرقان، 63"},
    {"text": "فَمَن يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُ", "source": "الزلزلة، 7"},
    {"text": "وَأَطِيعُوا اللَّهَ وَرَسُولَهُ", "source": "الأنفال، 1"},
    {"text": "وَمَا أَرْسَلْنَاكَ إِلَّا رَحْمَةً لِّلْعَالَمِينَ", "source": "الأنبياء، 107"},
    {"text": "وَاللَّهُ يَرْزُقُ مَن يَشَاءُ بِغَيْرِ حِسَابٍ", "source": "البقرة، 212"},
    {"text": "لَا تَقْنَطُوا مِن رَّحْمَةِ اللَّهِ", "source": "الزمر، 53"},
    {"text": "هَلْ جَزَاءُ الْإِحْسَانِ إِلَّا الْإِحْسَانُ", "source": "الرحمن، 60"},
    {"text": "اتَّقُوا اللَّهَ وَقُولُوا قَوْلًا سَدِيدًا", "source": "الأحزاب، 70"},
    {"text": "كُلُّ نَفْسٍ ذَائِقَةُ الْمَوْتِ", "source": "آل عمران، 185"},
    {"text": "وَاعْبُدْ رَبَّكَ حَتَّىٰ يَأْتِيَكَ الْيَقِينُ", "source": "الحجر، 99"},
    {"text": "وَهُوَ الْغَفُورُ الْوَدُودُ", "source": "البروج، 14"}
  ],
  
  "prayers": [
    {"text": "رَبِّ اغْفِرْ لِي وَلِوَالِدَيَّ وَلِلْمُؤْمِنِينَ", "source": "إبراهيم، 41"},
    {"text": "يَا مُقَلِّبَ الْقُلُوبِ ثَبِّتْ قَلْبِي عَلَى دِينِكَ", "source": "حديث"},
    {"text": "رَبِّ اشْرَحْ لِي صَدْرِي وَيَسِّرْ لِي أَمْرِي", "source": "طه، 25-26"},
    {"text": "اللَّهُمَّ إِنِّي أَسْأَلُكَ عِلْمًا نَافِعًا وَرِزْقًا طَيِّبًا وَعَمَلًا مُتَقَبَّلًا", "source": "حديث"},
    {"text": "رَبِّ اجْعَلْنِي مِنَ الشَّاكِرِينَ", "source": "النمل، 19"},
    {"text": "اللَّهُمَّ إِنَّكَ عَفُوٌّ تُحِبُّ الْعَفْوَ فَاعْفُ عَنِّي", "source": "حديث"},
    {"text": "رَبِّ لَا تَذَرْنِي فَرْدًا وَأَنتَ خَيْرُ الْوَارِثِينَ", "source": "الأنبياء، 89"},
    {"text": "اللَّهُمَّ إِنِّي أَسْأَلُكَ الْهُدَى وَالتُّقَى وَالْعَفَافَ وَالْغِنَى", "source": "حديث"},
    {"text": "رَبِّ أَلْحِقْنِي بِالصَّالِحِينَ", "source": "الشعراء، 83"},
    {"text": "اللَّهُمَّ أَصْلِحْ لِي قَلْبِي وَعَمَلِي", "source": "حديث"},
    {"text": "رَبِّ عَامِلْنِي بِرَحْمَتِكَ", "source": "الكهف، 10"},
    {"text": "اللَّهُمَّ عَافِنِي فِي دِينِي وَدُنْيَايَ وَآخِرَتِي", "source": "حديث"},
    {"text": "رَبِّ زِدْنِي عِلْمًا", "source": "طه، 114"},
    {"text": "اللَّهُمَّ إِنِّي أَعُوذُ بِكَ مِنَ الْعَجْزِ وَالْكَسَلِ وَالْجُبْنِ", "source": "حديث"},
    {"text": "رَبِّ اجْعَلْنِي مُقِيمَ الصَّلَاةِ وَمِن ذُرِّيَّتِي", "source": "إبراهيم، 40"},
    {"text": "اللَّهُمَّ اغْفِرْ لِي ذُنُوبِي وَوَسِّعْ لِي رِزْقِي", "source": "حديث"},
    {"text": "رَبَّنَا آتِنَا فِي الدُّنْيَا حَسَنَةً وَفِي الْآخِرَةِ حَسَنَةً وَقِنَا عَذَابَ النَّارِ", "source": "البقرة، 201"},
    {"text": "اللَّهُمَّ أَعِنِّي عَلَى فِعْلِ الْخَيْرَاتِ", "source": "حديث"},
    {"text": "رَبِّ تَوَفَّنِي مُسْلِمًا وَأَلْحِقْنِي بِالصَّالِحِينَ", "source": "يوسف، 101"},
    {"text": "اللَّهُمَّ إِنِّي أَسْأَلُكَ الصِّحَّةَ وَالْعَافِيَةَ", "source": "حديث"},
    {"text": "رَبِّ احْفَظْنِي مِنَ الشَّرِّ", "source": "غافر، 9"},
    {"text": "اللَّهُمَّ بَارِكْ لِي فِي خَيْرِي", "source": "حديث"},
    {"text": "حَسْبِيَ اللَّهُ لَا إِلَٰهَ إِلَّا هُوَ عَلَيْهِ تَوَكَّلْتُ", "source": "التوبة، 129"},
    {"text": "اللَّهُمَّ أَنْتَ رَبِّي لَا إِلَٰهَ إِلَّا أَنْتَ", "source": "سيد الاستغفار"},
    {"text": "رَبِّ أَعُوذُ بِكَ مِنْ هَمَزَاتِ الشَّيَاطِينِ", "source": "المؤمنون، 97"},
    {"text": "اللَّهُمَّ أَحْسِنْ حَالِي", "source": "حديث"},
    {"text": "رَبَّنَا أَفْرِغْ عَلَيْنَا صَبْرًا وَتَوَفَّنَا مُسْلِمِينَ", "source": "الأعراف، 126"},
    {"text": "اللَّهُمَّ أَنْتَ السَّلَامُ وَمِنْكَ السَّلَامُ", "source": "حديث"},
    {"text": "رَبِّ وَمَن يَغْفِرُ الذُّنُوبَ إِلَّا أَنتَ", "source": "آل عمران، 135"},
    {"text": "اللَّهُمَّ اخْتِمْ لِي بِخَيْرٍ", "source": "حديث"}
  ],
  
  "hadiths": [
    {"text": "إِنَّمَا الأَعْمَالُ بِالنِّيَّاتِ وَإِنَّمَا لِكُلِّ امْرِئٍ مَا نَوَى", "source": "البخاري، مسلم"},
    {"text": "الْمُسْلِمُ مَنْ سَلِمَ الْمُسْلِمُونَ مِنْ لِسَانِهِ وَيَدِهِ", "source": "البخاري، مسلم"},
    {"text": "يَسِّرُوا وَلَا تُعَسِّرُوا وَبَشِّرُوا وَلَا تُنَفِّرُوا", "source": "البخاري، مسلم"},
    {"text": "إِنَّ مِنْ خِيَارِكُمْ أَحْسَنَكُمْ أَخْلَاقًا", "source": "البخاري"},
    {"text": "الْكَلِمَةُ الطَّيِّبَةُ صَدَقَةٌ", "source": "البخاري، مسلم"},
    {"text": "الْجَنَّةُ تَحْتَ أَقْدَامِ الْأُمَّهَاتِ", "source": "النسائي"},
    {"text": "لَا يُؤْمِنُ أَحَدُكُمْ حَتَّى يُحِبَّ لِأَخِيهِ مَا يُحِبُّ لِنَفْسِهِ", "source": "البخاري، مسلم"},
    {"text": "الطُّهُورُ شَطْرُ الْإِيمَانِ", "source": "مسلم"},
    {"text": "الْمُؤْمِنُ الْقَوِيُّ خَيْرٌ مِنَ الْمُؤْمِنِ الضَّعِيفِ", "source": "مسلم"},
    {"text": "مَنْ كَانَ يُؤْمِنُ بِاللَّهِ وَالْيَوْمِ الْآخِرِ فَلَا يُؤْذِ جَارَهُ", "source": "البخاري، مسلم"},
    {"text": "مَنْ كَانَ يُؤْمِنُ بِاللَّهِ وَالْيَوْمِ الْآخِرِ فَلْيَقُلْ خَيْرًا أَوْ لِيَصْمُتْ", "source": "البخاري، مسلم"},
    {"text": "الدُّنْيَا مَزْرَعَةُ الْآخِرَةِ", "source": "الديلمي"},
    {"text": "طَلَبُ الْعِلْمِ فَرِيضَةٌ عَلَى كُلِّ مُسْلِمٍ", "source": "ابن ماجه"},
    {"text": "اطْلُبُوا الْعِلْمَ مِنَ الْمَهْدِ إِلَى اللَّحْدِ", "source": "حديث شريف"},
    {"text": "الْحِكْمَةُ ضَالَّةُ الْمُؤْمِنِ أَنَّى وَجَدَهَا فَهُوَ أَحَقُّ بِهَا", "source": "الترمذي"},
    {"text": "خَيْرُ النَّاسِ أَنْفَعُهُمْ لِلنَّاسِ", "source": "الطبراني"},
    {"text": "الصَّبْرُ مُرٌّ وَثَمَرَتُهُ حُلْوَةٌ", "source": "حديث شريف"},
    {"text": "تَفَكُّرُ سَاعَةٍ خَيْرٌ مِنْ عِبَادَةِ سَنَةٍ", "source": "حديث شريف"},
    {"text": "تَبَسُّمُكَ فِي وَجْهِ أَخِيكَ صَدَقَةٌ", "source": "الترمذي"},
    {"text": "إِذَا غَضِبْتَ فَاسْكُتْ", "source": "أحمد بن حنبل"},
    {"text": "إِمَاطَةُ الْأَذَى عَنِ الطَّرِيقِ صَدَقَةٌ", "source": "مسلم"},
    {"text": "كُلُّ مَعْرُوفٍ صَدَقَةٌ", "source": "البخاري"},
    {"text": "نِعْمَتَانِ مَغْبُونٌ فِيهِمَا كَثِيرٌ مِنَ النَّاسِ: الصِّحَّةُ وَالْفَرَاغُ", "source": "البخاري"},
    {"text": "اغْتَنِمْ خَمْسًا قَبْلَ خَمْسٍ: حَيَاتَكَ قَبْلَ مَوْتِكَ...", "source": "الحاكم"},
    {"text": "اللَّهُ فِي عَوْنِ الْعَبْدِ مَا كَانَ الْعَبْدُ فِي عَوْنِ أَخِيهِ", "source": "مسلم"},
    {"text": "الصَّلَاةُ عِمَادُ الدِّينِ", "source": "الترمذي"},
    {"text": "الصَّلَاةُ مِعْرَاجُ الْمُؤْمِنِ", "source": "حديث شريف"},
    {"text": "أَفْضَلُ الْجِهَادِ كَلِمَةُ حَقٍّ عِنْدَ سُلْطَانٍ جَائِرٍ", "source": "أبو داود"},
    {"text": "إِذَا لَمْ تَسْتَحْيِ فَاصْنَعْ مَا شِئْتَ", "source": "البخاري"},
    {"text": "الْوَرَعُ رَأْسُ الدِّينِ", "source": "حديث شريف"}
  ]
}
